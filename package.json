{
  "name": "vue-pc-template",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build:test": "vue-tsc -b && vite build --mode test",
    "build:pre": "vue-tsc -b && vite build --mode pre",
    "build:prod": "vue-tsc -b && vite build --mode prod",
    "preview": "pnpm build:test && vite preview",
    "report": "vue-tsc -b && vite build --mode prod",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "vue-tsc -b --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@types/lodash-es": "^4.17.12",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.13.2",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.3",
    "tailwind-merge": "^3.3.1",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "xy-config": "^1.0.0",
    "xy-enum": "^1.4.4",
    "xy-http": "^1.0.0",
    "xy-storage": "^3.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@iconify-json/tdesign": "^1.2.10",
    "@iconify/tailwind": "^1.2.0",
    "@iconify/tailwind4": "^1.0.6",
    "@tailwindcss/postcss": "^4.1.14",
    "@thedutchcoder/postcss-rem-to-px": "^0.0.2",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.37.0",
    "eslint-plugin-format": "^1.0.2",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.3",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.4",
    "sass": "^1.93.2",
    "tailwindcss": "^4.1.14",
    "typescript": "5.9.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^7.1.9",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vue-tsc": "^3.1.1"
  },
  "lint-staged": {
    "*.{ts,vue,js,json,scss}": [
      "pnpm lint"
    ]
  }
}
