    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "eslint-plugin-format": "^1.0.1",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "lint-staged": "^16.1.6",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.91.0",
    "tailwindcss": "^4.1.13",
    "typescript": "5.8.3",
    "unplugin-auto-import": "^20.1.0",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.1.4",
    "vite-plugin-vue-devtools": "^8.0.1",
    "vue-tsc": "^3.0.6"
