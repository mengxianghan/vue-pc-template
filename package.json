{
  "name": "vue-pc-template",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build:test": "vue-tsc -b && vite build --mode test",
    "build:pre": "vue-tsc -b && vite build --mode pre",
    "build:prod": "vue-tsc -b && vite build --mode prod",
    "preview": "pnpm build:test && vite preview",
    "report": "vue-tsc -b && vite build --mode prod",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.12",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.3",
    "tdesign-vue-next": "^1.16.1",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1",
    "xy-enum": "^1.4.4",
    "xy-http": "^1.0.0",
    "xy-storage": "^3.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@iconify-json/tabler": "^1.2.22",
    "@iconify-json/tdesign": "^1.2.8",
    "@iconify/tailwind": "^1.2.0",
    "@iconify/tailwind4": "^1.0.6",
    "@tailwindcss/postcss": "^4.1.13",
    "@tdesign-vue-next/auto-import-resolver": "^0.1.1",
    "@thedutchcoder/postcss-rem-to-px": "^0.0.2",
